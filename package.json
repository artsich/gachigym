{
  "name": "gachigym",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!axios|antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$",
      "\\.module\\.(css)"
    ],
    "moduleNameMapper": {}
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd-mobile": "^5.37.1",
    "antd-mobile-icons": "^0.3.0",
    "react": "^18.3.1",
    "react-activity-calendar": "^2.5.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.28.0",
    "uuid": "^10.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "^4.9.5"
  }
}
